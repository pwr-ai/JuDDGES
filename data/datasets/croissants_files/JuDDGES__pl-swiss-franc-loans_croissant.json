    {
      "@type": "cr:RecordSet",
      "@id": "default",
      "description": "JuDDGES/pl-swiss-franc-loans - 'default' subset\n\nAdditional information:\n- 3 splits: train, test, annotated",
      "field": [
        {
          "@type": "cr:Field",
          "@id": "default/split",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "fileProperty": "fullpath"
            },
            "transform": {
              "regex": "default/(?:partial-)?(train|test|annotated)/.+parquet$"
            }
          },
          "references": {
            "field": {
              "@id": "default_splits/split_name"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "default/context",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "context"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "default/output",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "parquet-files-for-config-default"
            },
            "extract": {
              "column": "output"
            }
          }
        }
      ]
    }
